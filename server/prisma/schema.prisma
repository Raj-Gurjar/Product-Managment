generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Product {
  id            String    @id @default(uuid())
  orderId       Int
  title         String
  description   String?
  quantity      Int
  totalPrice    Float
  totalDiscount Float
  createdAt     DateTime  @default(now())
  updatedAt     DateTime  @updatedAt
  deletedAt     DateTime?
}
